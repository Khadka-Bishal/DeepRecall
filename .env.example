# ==========================================
# DeepRecall Environment Variables
# ==========================================

# --- Frontend & API Config ---
VITE_API_URL=...
ALLOWED_ORIGINS=...

# --- Security & Auth (Required) ---
OPENAI_API_KEY=sk-proj-...
PINECONE_API_KEY=pcsk_...

# --- Vector Database ---
USE_PINECONE=true
PINECONE_INDEX_NAME=deeprecall
ENABLE_HYBRID_SEARCH=true
ENABLE_RERANKER=true

# --- AWS Configuration (Required) ---
AWS_REGION=...
AWS_ACCESS_KEY_ID=...
AWS_SECRET_ACCESS_KEY=...
DEEPRECALL_INPUT_BUCKET=deeprecall-input-bucket-name
DEEPRECALL_OUTPUT_BUCKET=deeprecall-output-bucket-name

# --- Infrastructure Names ---
DEEPRECALL_SQS_QUEUE=your-sqs-queue-name
DEEPRECALL_DLQ_QUEUE=your-sqs-dlq-name
DEEPRECALL_LAMBDA_NAME=your-lambda-function-name
DEEPRECALL_LAMBDA_ROLE=your-lambda-execution-role

# --- Observability (Optional) ---
LANGCHAIN_TRACING_V2=...
LANGCHAIN_API_KEY=lsv2_...
LANGSMITH_PROJECT=...
WANDB_API_KEY=...
WANDB_PROJECT=...
